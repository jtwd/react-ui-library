{
  "name": "devenv",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "highlight.js": "9.10.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "jest-styled-components": "^4.3.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "15.5.1",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-google-charts": "^1.5.5",
    "react-hot-loader": "next",
    "react-icons": "^2.2.5",
    "react-key-index": "^0.1.1",
    "react-loading": "^0.1.4",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "15.5.4",
    "reactable": "^0.14.1",
    "recompose": "^0.25.0",
    "shortid": "^2.2.8",
    "style-loader": "0.18.2",
    "styled-components": "^2.1.1",
    "sw-precache-webpack-plugin": "0.11.3",
    "tinycolor2": "^1.4.1",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "prestart": "npm run gen:docs",
    "start": "node scripts/start.js",
    "docs": "npm-run-all --parallel start gen:docs-watch",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "format": "prettier --write --no-semi --single-quote --print-width=180 --tab-width=2 \"src/**/*.{js,jsx}\"",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "npm run test -- --coverage"
  },
  "devDependencies": {
    "chokidar": "1.6.1",
    "enzyme-to-json": "^1.5.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-prettier": "^2.3.0",
    "node-sass": "^4.5.3",
    "normalize": "^0.3.1",
    "npm-run-all": "4.0.2",
    "prettier": "^1.5.2",
    "react-docgen": "2.14.0",
    "sass-loader": "^6.0.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/components/_docs/*.{js,jsx}",
      "!src/components/**/examples/*.js?(x)",
      "!src/registerServiceWorker.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "linebreak-style": 0
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    }
  }
}
